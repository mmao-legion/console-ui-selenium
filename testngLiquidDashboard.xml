<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite verbose ="0" thread-count="10" name="LegionTest">
    <parameter name="environment" value="QA"/>
    <parameter name="testRail" value="yes"/>
    <parameter name="testSuiteName" value="Cinemark Minors"/>
    <listeners>
        <listener class-name="com.legion.tests.testframework.LegionTestListener"></listener>
    </listeners>

    <test name="Cinemark Minor Test">
        <classes>
            <class name="com.legion.tests.core.CinemarkMinorTest">
                <methods>
                    <include name="verifyAccessToEditCalendarsAsInternalAdmin" />
                    <include name="verifyTurnOffMinorRuleAsInternalAdmin" />
                    <include name="verifyTurnOnAndSetMinorRuleEmptyAsInternalAdmin" />
                </methods>
            </class>
        </classes>
    </test>
</suite>
