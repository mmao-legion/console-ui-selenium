<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite verbose ="0" thread-count="10" name="LegionTest">
	<parameter name="environment" value="QA"/>
  	
    <listeners>
 		<listener class-name="com.legion.tests.testframework.LegionTestListener"></listener>
 	</listeners>
   
  <test name="Traffic Forecast for New UI Test">
    <classes>
       <class name="com.legion.tests.core.TrafficForecastTest">
      	<methods>
          <include name="trafficForecastDataAsStoreManagerTest"/> 
        </methods>
      </class>
    </classes>
  </test>  
  
  <test name="Staffing Guidance for New UI Test">
    <classes>
       <class name="com.legion.tests.core.StaffingGuidanceTestKendraScott2">
      	<methods>
          <include name="staffingGuidanceDataAsStoreManagerTestKendraScott2"/> 
        </methods>
      </class>
    </classes>
  </test>  
  
  <test name="Controls New UI Test">
    <classes>
       <class name="com.legion.tests.core.ControlsNewUITest">
      	<methods>
          <include name="updateUserLocationAsInternalAdmin"/> 
           <include name="updateWorkingHoursAsInternalAdmin"/> 
        </methods>
      </class>
    </classes>
  </test>  
  
  
  
</suite> <!-- Suite -->




