<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite verbose ="0" thread-count="10" name="LegionTest">
	<parameter name="environment" value="QA"/>
    <parameter name="platform" value="Android11"/>
    <parameter name="executionon" value="realdevice"/>
    <parameter name="runMode" value="mobileAndWeb"/>


    <listeners>
 		<listener class-name="com.legion.tests.testframework.LegionTestListener"></listener>
 	</listeners>


    <test name="Team New UI Test">
        <classes>
            <class name="com.legion.tests.core.NavigationTest">
                <methods>
                    <include name="scheduleSubTabNavigationStoreManager"/>
                    <include name="projectedTrafficSubTabNavigationStoreManager"/>
                    <include name="staffingGuidanceSubTabNavigationStoreManager"/>
                </methods>
            </class>
        </classes>
    </test>

</suite> <!-- Suite -->




