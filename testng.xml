<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite verbose ="0" thread-count="5" name="LegionTest">
	<parameter name="environment" value="QA"/>
  	<parameter name="enterprise" value="LegionCoffee2"/>
    <parameter name="browser" value="Chrome"/>
    <listeners>
 		<listener class-name="com.legion.tests.testframework.LegionTestListener"></listener>
 	</listeners>
   
  
  
  <test name="Schedule Test">      
    <classes>
       <class name="com.legion.tests.core.ScheduleTest">
      	<methods>
            
             <include name="hoursAndWagesCalculationOnSchedulePage"/>
             <include name="reviewPastGenerateCurrentAndFutureWeekSchedule"/> 
            
        </methods>
      </class>
    </classes>
  </test> <!-- Test -->
  
 
  
  <test name="Navigation Flow">      
    <classes>
       <class name="com.legion.tests.core.NavigationTest">
      	<methods>
           <include name="legionConsoleNavigationFlow"/>
           <include name="DataNotLoadingForCarmelClubLocation"/>
        </methods>
      </class>
    </classes>
  </test> <!-- Test -->
  
  
  <test name="Analytics Test">      
    <classes>
       <class name="com.legion.tests.core.AnalyticsTest">
      	<methods>
           <include name="gotoAnalyticsPageTest"/>
        </methods>
      </class>
    </classes>
  </test> <!-- Test -->
  
  
  
   
</suite> <!-- Suite -->

