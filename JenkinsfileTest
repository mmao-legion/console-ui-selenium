pipeline {
   agent any
   stages{
    stage('Checkout') {
       steps{
        checkout scm
       }
    }
    stage('Ops Portal') {
         steps{
         catchError(buildResult: 'SUCCESS', stageResult: 'FAILURE') {
            sh 'mvn clean test -Dsurefire.suiteXmlFiles=$xmlFileName -Denv=$env -Denterprise=$enterprise '
         }
     publishHTML([allowMissing: false, alwaysLinkToLastBuild: false, keepAll: true, reportDir: 'test-output/AutomationReport', reportFiles: 'htmlReporter.html', reportName: 'OpsPortal_Report', reportTitles: 'Locations Tests'])
        }
    }

  }