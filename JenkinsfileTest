pipeline {
   agent any
   stages{
    stage('Checkout') {
       steps{
        checkout scm
       }
    }
    stage('Ops Portal') {
         steps{
         catchError(buildResult: 'SUCCESS', stageResult: 'FAILURE') {
            sh 'mvn clean -DsuiteXmlFiles=testng.xml'
         }

        }
    }

  }

  }