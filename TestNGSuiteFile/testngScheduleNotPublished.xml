<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite verbose ="0" thread-count="10" name="LegionTest">
    <parameter name="environment" value="QA"/>
    <parameter name="testRail" value="yes"/>
    <parameter name="testSuiteName" value="Schedule Not Published"/>
    <listeners>
        <listener class-name="com.legion.tests.testframework.LegionTestListener"></listener>
    </listeners>

    <test name="Schedule New UI Test">
        <classes>
            <class name="com.legion.tests.core.ScheduleTestKendraScott2">
                <methods>
                    <include name="verifySmartCardForScheduleNotPublishAsInternalAdmin" />
                    <include name="verifyNumberOnSmartCardForScheduleNotPublishAsInternalAdmin" />
                    <include name="verifyComplianceViolationWhenScheduleIsNotPublishedAsInternalAdmin" />
                    <include name="verifyComplianceViolationWhenScheduleHasPublishedAsInternalAdmin" />
<!--                    <include name="verifyTooltipForDeleteShiftsAsInternalAdmin" />-->
                    <include name="verifyUnpublishedEditsTextOnDashboardAndOverviewPageAsInternalAdmin" />
                </methods> 
            </class>
        </classes>
    </test>

</suite>
